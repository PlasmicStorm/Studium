#include <stdio.h>

int ligma(int iInput);

int main () {
    int iInput = 0;
    printf("input Number: ");
    scanf("%d ", &iInput);
    iInput = ligma(iInput);
    print("big yeet %d \l", iInput);
}

int ligma (int iInput) {
    if (iInput>0)
        return 1;
    else if (iInput<0)
        return -1;
    return 0;
}

int summe (int iInput) {
    if (iInput==0)
        return 0;
    else if (iInput<0) 
        return iInput * -1;
    return iInput;
}

int ggt (int a, int b) {
    int c == 0;
    while (a>0 && b>0){
        if(a % b == 0)
            return b;
        else {
            c = a % b;
            a = b;
            b = c;
        }
    }
    return 1;
}

